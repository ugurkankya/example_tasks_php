{
    "terminal.integrated.shell.linux": "/bin/bash",
    "workbench.colorTheme": "Default Dark+",
    "editor.minimap.enabled": false,
    "editor.renderIndentGuides": false,
    "editor.folding": false,
    "editor.renderLineHighlight": "line",
    "workbench.colorCustomizations": {
        "editor.lineHighlightBackground": "#37373d"
    },
    "workbench.editor.enablePreview": false,
    "workbench.startupEditor": "none",
    "explorer.openEditors.visible": 0,
    "breadcrumbs.enabled": false,
    "extensions.autoUpdate": false,
    "php.suggest.basic": false,
    "git.enabled": false,
    "vscode-php-cs-fixer.toolPath": "/var/www/bin/phpcsfixer-2173.phar",
    "vscode-php-cs-fixer.config": "/var/www/phpcsfixer.conf",
    "vscode-php-cs-fixer.fixOnSave": false,
    "[php]": { "editor.defaultFormatter": "fterrag.vscode-php-cs-fixer" },
    "intelephense.format.enable": false,
    "intelephense.telemetry.enabled": false,
    "files.exclude": {
        ".bash*": true,
        ".buildpath": true,
        ".gitignore": true,
        ".local": true,
        ".project": true,
        ".settings": true,
        "tests/coverage/": true
    }
